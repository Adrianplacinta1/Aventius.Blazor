@inherits TriangleBase

<svg width="100%" height="100%">
    <polygon points="@X1,@Y1 @X2,@Y2 @X3,@Y3" class="@Class" style="@InternalStyle"
             @onmousedown="@OnMouseDown"
             @onmouseup="@OnMouseUp"
             @onmouseover="@OnMouseOver"
             @onmouseout="@OnMouseOut"
             @onclick="@OnClick" />
</svg>